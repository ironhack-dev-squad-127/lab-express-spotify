<h1>Tracks</h1>


{{#each tracks}}
    <div class="my-5">
      <h5>{{this.name}}</h5>
      {{#if this.preview_url}}
          <audio controls>
            <source src="{{this.preview_url}}" type="audio/mp3">
          </audio>
      {{else}}
        Sorry, no preview
      {{/if}}
    </div>
{{/each}}